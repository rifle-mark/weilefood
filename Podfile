platform :ios, '7.0'
#use_frameworks!

target 'Weilefood' do
  # 友盟统计
  pod 'UMengAnalytics'
  # 友盟分享
  ##pod 'UMengSocial'

  # 网络请求库
  pod 'AFNetworking'
  pod 'AFNetworkActivityLogger', :configurations => ['Debug']
  # AutoLayout库
  pod 'Masonry'
  # JSON与Model对象互转库
  pod 'MJExtension'
  # 网络图片显示，以及缓存
  pod 'SDWebImage'
  # HUD提示组件
  pod 'MBProgressHUD'
  # 下拉、上拉刷新功能
  pod 'MJRefresh'
  # CoreData扩展
  pod 'MagicalRecord'
  # 方法拦截
  pod 'Aspects'

end

